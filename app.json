{
  "name": "mnf-squares",
  "env": {
    "APP_JWT_SECRET": {
      "description": "JWT secret",
      "generator": "secret"
    },
    "NEXTAUTH_URL": {
      "description": "App base URL",
      "required": true
    },
    "SQUARE_PRICE_CENTS": {
      "description": "Price per square in cents",
      "value": "2000"
    }
  },
  "scripts": {
    "postdeploy": "npx prisma migrate deploy"
  },
  "addons": [
    "heroku-postgresql:hobby-dev"
  ]
}
